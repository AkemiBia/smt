{
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/counters",
      "destination": "/api/counters.js"
    },
    {
      "source": "/api/increment",
      "destination": "/api/increment.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
